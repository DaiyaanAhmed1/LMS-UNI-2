# AI Service Configuration
# Copy this file to .env and add your actual API keys

# Choose which AI to use: 'true' for the selected service, 'false' for others
VITE_USE_GEMINI=false
VITE_USE_DEEPSEEK=true

# OpenRouter API Key (for DeepSeek models) - https://openrouter.ai/
VITE_DEEPSEEK_API_KEY=


# Gemini API Key (Google AI Studio)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Key (alternative)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Rate Limiting (requests per hour)
VITE_FREE_TIER_LIMIT=100
VITE_PRO_TIER_LIMIT=1000

# Feature Flags
VITE_ENABLE_REAL_AI=true
VITE_ENABLE_ARABIC_SUPPORT=true 